= form_for @credential do |f|
  -if @credential.errors.any?
    #error_explanation
      %h2= "#{pluralize(@credential.errors.count, "error")} prohibited this credential from being saved:"
      %ul
        - @credential.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :email
    = f.text_field :email
  .field
    = f.label :password
    = f.text_field :password
  .field
    = f.label :developer_token
    = f.text_field :developer_token
  .actions
    = f.submit 'Save'
